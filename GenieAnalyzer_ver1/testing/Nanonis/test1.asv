%alternative way to open file_data

file = 'Sweeper00016.dat';
fid = fopen(file);
data = textscan(fid, '%s', 'Delimiter', '\n', 'CollectOutput', true);

Data = data{1,1};

for i=1:length(Data)
    str = Data{i,1};
    if(strcmp(str,'[DATA]'))
        index = i
        break;    
    end
end

cnt = 1;
for i=index+2:length(Data)
    nums(cnt,:) = strsplit(Data{i,1},'\t');
    cnt = c
end
